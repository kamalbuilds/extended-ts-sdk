{
  "name": "@starkent-defi-protocols/extended-ts-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Starknet DeFi protocols with perpetual trading support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint **/*.ts --ignore-pattern node_modules --ignore-pattern dist",
    "lint:fix": "eslint **/*.ts --ignore-pattern node_modules --ignore-pattern dist --fix",
    "format": "prettier --write **/*.ts --ignore-path .gitignore",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "dev": "ts-node src/index.ts",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:trading": "ts-node examples/trading-example.ts",
    "example:onboarding": "ts-node examples/onboarding-example.ts"
  },
  "keywords": [
    "starknet",
    "defi",
    "perpetual",
    "trading",
    "blockchain",
    "typescript",
    "l2"
  ],
  "author": "Starkent DeFi Protocols",
  "license": "MIT",
  "dependencies": {
    "starknet": "^7.6.4",
    "@scure/starknet": "^1.0.0",
    "ethers": "^6.13.4",
    "ws": "^8.18.0",
    "axios": "^1.7.7",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starkent-defi-protocols/extended-ts-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/starkent-defi-protocols/extended-ts-sdk/issues"
  },
  "homepage": "https://github.com/starkent-defi-protocols/extended-ts-sdk#readme"
}